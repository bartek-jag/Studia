<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="zamowienie">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="cena" type="xsd:nonNegativeInteger" minOccurs="1" maxOccurs="1"/>
				<xsd:element name="adres" type="xsd:string" maxOccurs="1"/>
				<xsd:element name="kontakt" type="xsd:string"/>
				<xsd:element name="towar" type="xsd:string" minOccurs="1" maxOccurs="12"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>

