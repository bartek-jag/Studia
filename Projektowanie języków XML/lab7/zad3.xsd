<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:element name="glowny">
        <xsd:complexType mixed="true">
            <xsd:sequence>
                <xsd:element name="element1" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="element2" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>

